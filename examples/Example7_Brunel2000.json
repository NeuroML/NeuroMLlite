{
    "Example7_Brunel2000": {
        "version": "NeuroMLlite v0.1.9", 
        "parameters": {
            "epsilon": 0.1, 
            "J": 0.1, 
            "U0": 0.0, 
            "g": 4, 
            "tauMem": 20.0, 
            "tauRef": 2.0, 
            "delay": 1.5, 
            "eta": 1, 
            "theta": 20.0, 
            "tauSyn": 0.1, 
            "order": 5
        }, 
        "notes": "Example 7: based on network of Brunel 2000", 
        "cells": [
            {
                "ifcell": {
                    "pynn_cell": "IF_curr_alpha", 
                    "parameters": {
                        "tau_refrac": "tauRef", 
                        "tau_m": "tauMem", 
                        "i_offset": 0, 
                        "cm": 0.001, 
                        "v_thresh": "theta", 
                        "v_rest": "U0", 
                        "v_reset": "U0"
                    }
                }
            }, 
            {
                "poisson_input": {
                    "pynn_cell": "SpikeSourcePoisson", 
                    "parameters": {
                        "duration": 1000000000.0, 
                        "start": 0, 
                        "rate": "1000 * (eta*theta/(J*4*order*epsilon*tauMem)) * (4*order*epsilon)"
                    }
                }
            }
        ], 
        "synapses": [
            {
                "ampa": {
                    "pynn_synapse_type": "curr_alpha", 
                    "pynn_receptor_type": "excitatory", 
                    "parameters": {
                        "tau_syn": 0.1
                    }
                }
            }, 
            {
                "gaba": {
                    "pynn_synapse_type": "curr_alpha", 
                    "pynn_receptor_type": "inhibitory", 
                    "parameters": {
                        "tau_syn": 0.1
                    }
                }
            }
        ], 
        "regions": [
            {
                "region1": {
                    "x": 0.0, 
                    "y": 0.0, 
                    "z": 0.0, 
                    "width": 1000.0, 
                    "height": 100.0, 
                    "depth": 1000.0
                }
            }
        ], 
        "populations": [
            {
                "Epop": {
                    "size": "4*order", 
                    "component": "ifcell", 
                    "properties": {
                        "color": ".9 0 0", 
                        "radius": 5
                    }, 
                    "random_layout": {
                        "region": "region1"
                    }
                }
            }, 
            {
                "expoisson": {
                    "size": "4*order", 
                    "component": "poisson_input", 
                    "properties": {
                        "color": "0.9 0.7 0.7", 
                        "radius": 3
                    }, 
                    "random_layout": {
                        "region": "region1"
                    }
                }
            }, 
            {
                "Ipop": {
                    "size": "1*order", 
                    "component": "ifcell", 
                    "properties": {
                        "color": "0 0 .9", 
                        "radius": 5
                    }, 
                    "random_layout": {
                        "region": "region1"
                    }
                }
            }, 
            {
                "inpoisson": {
                    "size": "1*order", 
                    "component": "poisson_input", 
                    "properties": {
                        "color": "0.7 0.7 0.9", 
                        "radius": 3
                    }, 
                    "random_layout": {
                        "region": "region1"
                    }
                }
            }
        ], 
        "projections": [
            {
                "projEinput": {
                    "presynaptic": "expoisson", 
                    "postsynaptic": "Epop", 
                    "synapse": "ampa", 
                    "delay": 0.025, 
                    "weight": "((J*1)/tauSyn)*0.000413635066326", 
                    "one_to_one_connector": {}
                }
            }, 
            {
                "projIinput": {
                    "presynaptic": "inpoisson", 
                    "postsynaptic": "Ipop", 
                    "synapse": "ampa", 
                    "delay": 0.025, 
                    "weight": "((J*1)/tauSyn)*0.000413635066326", 
                    "one_to_one_connector": {}
                }
            }, 
            {
                "projEE": {
                    "presynaptic": "Epop", 
                    "postsynaptic": "Epop", 
                    "synapse": "ampa", 
                    "delay": "delay", 
                    "weight": "((J*1)/tauSyn)*0.000413635066326", 
                    "random_connectivity": {
                        "probability": "epsilon"
                    }
                }
            }, 
            {
                "projEI": {
                    "presynaptic": "Epop", 
                    "postsynaptic": "Ipop", 
                    "synapse": "ampa", 
                    "delay": "delay", 
                    "weight": "((J*1)/tauSyn)*0.000413635066326", 
                    "random_connectivity": {
                        "probability": "epsilon"
                    }
                }
            }, 
            {
                "projIE": {
                    "presynaptic": "Ipop", 
                    "postsynaptic": "Epop", 
                    "synapse": "gaba", 
                    "delay": "delay", 
                    "weight": "-1*g*((J*1)/tauSyn)*0.000413635066326", 
                    "random_connectivity": {
                        "probability": "epsilon"
                    }
                }
            }, 
            {
                "projII": {
                    "presynaptic": "Ipop", 
                    "postsynaptic": "Ipop", 
                    "synapse": "gaba", 
                    "delay": "delay", 
                    "weight": "-1*g*((J*1)/tauSyn)*0.000413635066326", 
                    "random_connectivity": {
                        "probability": "epsilon"
                    }
                }
            }
        ]
    }
}