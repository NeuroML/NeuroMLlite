{
    "Example7_Brunel2000": {
        "version": "NeuroMLlite v0.1.4", 
        "parameters": {
            "epsilon": 0.1, 
            "J": 0.1, 
            "U0": 0.0, 
            "g": 4, 
            "tauMem": 20.0, 
            "tauRef": 2.0, 
            "delay": 1.5, 
            "eta": 1, 
            "theta": 20.0, 
            "tauSyn": 0.1, 
            "order": 5
        }, 
        "notes": "Example 7: based on network of Brunel 2000", 
        "cells": [
            {
                "ifcell": {
                    "pynn_cell": "IF_curr_alpha", 
                    "parameters": {
                        "tau_refrac": "tauRef", 
                        "tau_m": "tauMem", 
                        "i_offset": 0, 
                        "cm": 0.001, 
                        "v_thresh": "theta", 
                        "v_rest": "U0", 
                        "v_reset": "U0"
                    }
                }
            }, 
            {
                "expoisson": {
                    "pynn_cell": "SpikeSourcePoisson", 
                    "parameters": {
                        "duration": 1000000000.0, 
                        "start": 0, 
                        "rate": "1000 * (eta*theta/(J*4*order*epsilon*tauMem)) * (4*order*epsilon)"
                    }
                }
            }
        ], 
        "synapses": [
            {
                "ampa": {
                    "pynn_synapse_type": "curr_alpha", 
                    "pynn_receptor_type": "excitatory", 
                    "parameters": {
                        "tau_syn": 0.1
                    }
                }
            }, 
            {
                "gaba": {
                    "pynn_synapse_type": "curr_alpha", 
                    "pynn_receptor_type": "inhibitory", 
                    "parameters": {
                        "tau_syn": 0.1
                    }
                }
            }
        ], 
        "populations": [
            {
                "Epop": {
                    "size": "4*order", 
                    "component": "ifcell", 
                    "properties": {
                        "color": "1 0 0"
                    }
                }
            }, 
            {
                "Einput": {
                    "size": "4*order", 
                    "component": "expoisson", 
                    "properties": {
                        "color": ".5 0 0"
                    }
                }
            }, 
            {
                "Ipop": {
                    "size": "1*order", 
                    "component": "ifcell", 
                    "properties": {
                        "color": "0 0 1"
                    }
                }
            }
        ], 
        "projections": [
            {
                "projEinput": {
                    "presynaptic": "Einput", 
                    "postsynaptic": "Epop", 
                    "synapse": "ampa", 
                    "delay": 2, 
                    "weight": 0.02, 
                    "one_to_one_connector": {}
                }
            }, 
            {
                "projEI": {
                    "presynaptic": "Epop", 
                    "postsynaptic": "Ipop", 
                    "synapse": "ampa", 
                    "delay": 2, 
                    "weight": 0.02, 
                    "random_connectivity": {
                        "probability": 0.5
                    }
                }
            }
        ]
    }
}