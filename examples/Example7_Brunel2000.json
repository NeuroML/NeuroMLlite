{
    "Example7_Brunel2000": {
        "version": "NeuroMLlite v0.3.8",
        "parameters": {
            "g": 4,
            "eta": 1,
            "order": 5,
            "epsilon": 0.1,
            "J": 0.1,
            "delay": 1.5,
            "tauMem": 20.0,
            "tauSyn": 0.1,
            "tauRef": 2.0,
            "U0": 0.0,
            "theta": 20.0
        },
        "notes": "Example 7: based on network of Brunel 2000",
        "cells": {
            "ifcell": {
                "pynn_cell": "IF_curr_alpha",
                "parameters": {
                    "tau_m": "tauMem",
                    "tau_refrac": "tauRef",
                    "v_rest": "U0",
                    "v_reset": "U0",
                    "v_thresh": "theta",
                    "cm": 0.001,
                    "i_offset": 0
                }
            },
            "poisson_input": {
                "pynn_cell": "SpikeSourcePoisson",
                "parameters": {
                    "rate": "1000 * (eta*theta/(J*4*order*epsilon*tauMem)) * (4*order*epsilon)",
                    "start": 0,
                    "duration": 1000000000.0
                }
            }
        },
        "synapses": {
            "ampa": {
                "pynn_synapse_type": "curr_alpha",
                "pynn_receptor_type": "excitatory",
                "parameters": {
                    "tau_syn": 0.1
                }
            },
            "gaba": {
                "pynn_synapse_type": "curr_alpha",
                "pynn_receptor_type": "inhibitory",
                "parameters": {
                    "tau_syn": 0.1
                }
            }
        },
        "regions": {
            "region1": {
                "x": 0.0,
                "y": 0.0,
                "z": 0.0,
                "width": 1000.0,
                "height": 100.0,
                "depth": 1000.0
            }
        },
        "populations": {
            "Epop": {
                "size": "4*order",
                "component": "ifcell",
                "properties": {
                    "color": ".9 0 0",
                    "radius": 5
                },
                "random_layout": {
                    "region": "region1"
                }
            },
            "expoisson": {
                "size": "4*order",
                "component": "poisson_input",
                "properties": {
                    "color": "0.9 0.7 0.7",
                    "radius": 3
                },
                "random_layout": {
                    "region": "region1"
                }
            },
            "Ipop": {
                "size": "1*order",
                "component": "ifcell",
                "properties": {
                    "color": "0 0 .9",
                    "radius": 5
                },
                "random_layout": {
                    "region": "region1"
                }
            },
            "inpoisson": {
                "size": "1*order",
                "component": "poisson_input",
                "properties": {
                    "color": "0.7 0.7 0.9",
                    "radius": 3
                },
                "random_layout": {
                    "region": "region1"
                }
            }
        },
        "projections": {
            "projEinput": {
                "presynaptic": "expoisson",
                "postsynaptic": "Epop",
                "synapse": "ampa",
                "delay": 0.025,
                "weight": "((J*1)/tauSyn)*0.00041363506632638",
                "one_to_one_connector": {}
            },
            "projIinput": {
                "presynaptic": "inpoisson",
                "postsynaptic": "Ipop",
                "synapse": "ampa",
                "delay": 0.025,
                "weight": "((J*1)/tauSyn)*0.00041363506632638",
                "one_to_one_connector": {}
            },
            "projEE": {
                "presynaptic": "Epop",
                "postsynaptic": "Epop",
                "synapse": "ampa",
                "delay": "delay",
                "weight": "((J*1)/tauSyn)*0.00041363506632638",
                "random_connectivity": {
                    "probability": "epsilon"
                }
            },
            "projEI": {
                "presynaptic": "Epop",
                "postsynaptic": "Ipop",
                "synapse": "ampa",
                "delay": "delay",
                "weight": "((J*1)/tauSyn)*0.00041363506632638",
                "random_connectivity": {
                    "probability": "epsilon"
                }
            },
            "projIE": {
                "presynaptic": "Ipop",
                "postsynaptic": "Epop",
                "synapse": "gaba",
                "delay": "delay",
                "weight": "-1*g*((J*1)/tauSyn)*0.00041363506632638",
                "random_connectivity": {
                    "probability": "epsilon"
                }
            },
            "projII": {
                "presynaptic": "Ipop",
                "postsynaptic": "Ipop",
                "synapse": "gaba",
                "delay": "delay",
                "weight": "-1*g*((J*1)/tauSyn)*0.00041363506632638",
                "random_connectivity": {
                    "probability": "epsilon"
                }
            }
        }
    }
}