# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation

target: test_ex7_pynn.py  
engine: PyNN_Nest 
mep: .test.ex7.mep
experiments:

  spike_rate_in_0:
    observables:
      spike rate:
        spiketimes file: 
          path: ../SimExample7.Einput.spikes
          format: ID_TIME
          id: 0
        tolerance: 0.021692670229483155
        
  spike_rate_in_1:
    observables:
      spike rate:
        spiketimes file: 
          path: ../SimExample7.Einput.spikes
          format: ID_TIME
          id: 1
        tolerance: 0.021949218900145204
        
  spike_rate_e_0:
    observables:
      spike rate:
        spiketimes file: 
          path: ../SimExample7.Epop.spikes
          format: ID_TIME
          id: 0
        tolerance: 0.0006715389044461795

