# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation

target: test_ex4_jnml.py 
engine: NetPyNE 
mep: .test.ex4.mep
experiments:
  v0:
    observables:
      spike times:
        file: 
          path: ../SimExample4.pop1.v.dat
          columns: [0,1]
          scaling: [1000, 1000]
        spike detection: 
          method: threshold
          threshold: -64
        tolerance: 0.00025021894157388664

  v1:
    observables:
      spike times:
        file: 
          path: ../SimExample4.pop2.v.dat
          columns: [0,1]
          scaling: [1000, 1000]
        spike detection: 
          method: threshold
          threshold: -69
        tolerance: 0.0002719276859891263
